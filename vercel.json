{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/teacher-dashboard",
      "destination": "/index.html"
    },
    { "source": "/student-dashboard", "destination": "/student-dashboard.html" }
  ]
}
