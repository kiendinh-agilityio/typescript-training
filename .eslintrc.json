{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "space-before-function-paren": "off",
    "no-new": "off",
    "quotes": "off",
    "no-undef": "off",
    "no-extra-boolean-cast": "off"
  }
}
